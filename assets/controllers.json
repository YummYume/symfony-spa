{
  "controllers": {
    "@symfony/ux-lazy-image": {
      "lazy-image": {
        "enabled": true,
        "fetch": "lazy"
      }
    },
    "@symfony/ux-live-component": {
      "live": {
        "enabled": true,
        "fetch": "lazy",
        "autoimport": {
          "@symfony/ux-live-component/styles/live.css": true
        }
      }
    },
    "@symfony/ux-turbo": {
      "turbo-core": {
        "enabled": true,
        "fetch": "eager"
      },
      "mercure-turbo-stream": {
        "enabled": true,
        "fetch": "eager"
      }
    }
  },
  "entrypoints": []
}
